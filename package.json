{
  "name": "@marufme/device-key",
  "private": false,
  "version": "1.1.0",
  "description": "A lightweight, modular TypeScript library for comprehensive device detection, fingerprinting, and information gathering in browsers. Features tree-shakeable modules for OS detection, browser analysis, network info, device fingerprinting, and more.",
  "type": "module",
  "author": {
    "name": "MD Maruf Hossain",
    "email": "marufme.dev@gmail.com",
    "url": "https://marufme.com"
  },
  "license": "MIT",
  "homepage": "https://doc-device-key.marufme.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/maruf-me/device-key.git",
    "directory": "packages/device-key"
  },
  "bugs": {
    "url": "https://github.com/maruf-me/device-key/issues"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/maruf-me"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "device-inspector",
    "device-analyzer",
    "device-checker",
    "device-insight",
    "device-identify",
    "device-fingerprint",
    "browser-detection",
    "os-detection",
    "device-info",
    "user-agent",
    "device-identification",
    "client-detection",
    "hardware-detection",
    "network-detection",
    "typescript",
    "tree-shakeable",
    "modular",
    "lightweight",
    "browser-api",
    "device-profiling",
    "client-side",
    "web-detection"
  ],
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "main": "./dist/device-key.cjs",
  "module": "./dist/device-key.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/device-key.js",
      "require": "./dist/device-key.cjs"
    },
    "./package.json": "./package.json"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "devDependencies": {
    "typescript": "~5.8.3",
    "vite": "^7.1.2",
    "@types/node": "^22.9.0",
    "vite-plugin-dts": "^4.3.0",
    "@changesets/cli": "^2.27.9"
  }
}
